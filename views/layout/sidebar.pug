aside.main-sidebar.sidebar-dark-primary.elevation-4    
    a(href="#").brand-link.elevation-4
        img(src="/img/AdminLTELogo.png" alt="AdminLTE Logo" style="opacity: .8").brand-image.img-circle.elevation-3
        span.brand-text.font-weight-light Dedications API
    div.sidebar
        div.user-panel.mt-3.pb-3.mb-3.d-flex
            div.image
                img.img-circle.elevation-2(src="/img/user2-160x160b.jpg" alt="User Image")
            div.info
                a.d-block(href="#") #{userName}
    nav.mt-2
    ul.nav.nav-pills.nav-sidebar.flex-column(data-widget="treeview" role="menu" data-accordion="false")
        li.nav-item
        a.nav-link.active(href="#")
            i.mr-2.nav-icon.far.fa-calendar-alt
            p Agenda
            span.badge.badge-info.right 2